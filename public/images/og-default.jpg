<!-- SVG Open Graph image placeholder - 1200x630 -->
<svg width="1200" height="630" viewBox="0 0 1200 630" fill="none" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="ogGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#033379"/>
      <stop offset="100%" style="stop-color:#02244f"/>
    </linearGradient>
  </defs>

  <!-- Background -->
  <rect width="1200" height="630" fill="url(#ogGrad)"/>

  <!-- Subtle pattern -->
  <pattern id="dots" x="0" y="0" width="30" height="30" patternUnits="userSpaceOnUse">
    <circle cx="15" cy="15" r="1" fill="white" fill-opacity="0.1"/>
  </pattern>
  <rect width="1200" height="630" fill="url(#dots)"/>

  <!-- Content container -->
  <g transform="translate(100, 200)">
    <!-- Company name -->
    <text x="0" y="0" font-family="Arial, sans-serif" font-size="48" font-weight="bold" fill="white">
      VP Associates
    </text>

    <!-- Tagline -->
    <text x="0" y="50" font-family="Arial, sans-serif" font-size="28" fill="white" fill-opacity="0.9">
      Structural Engineering Services
    </text>

    <!-- Accent line -->
    <rect x="0" y="80" width="200" height="4" fill="#BE0000"/>

    <!-- Subtitle -->
    <text x="0" y="130" font-family="Arial, sans-serif" font-size="24" fill="white" fill-opacity="0.8">
      Tampa Bay's Trusted Engineers for 30+ Years
    </text>
  </g>

  <!-- Bottom accent -->
  <rect x="0" y="580" width="1200" height="50" fill="#BE0000" fill-opacity="0.3"/>
</svg>