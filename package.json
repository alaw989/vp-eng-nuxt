{
  "name": "vp-eng-nuxt",
  "version": "1.0.0",
  "description": "VP Associates Website - Nuxt 3",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "start": "node .output/server/index.mjs",
    "postinstall": "nuxt prepare",
    "prepare": "husky",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "optimize:team": "tsx scripts/optimize-team-photos.ts",
    "lighthouse": "lhci autorun",
    "lighthouse:collect": "lhci collect --url=http://localhost:3000",
    "test:lighthouse": "npm run build && npm run preview & npm run lighthouse:collect && pkill -f preview",
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:unit:ui": "vitest --ui",
    "test:unit:coverage": "vitest run --coverage",
    "test:all": "npm run test:unit && npm run test:e2e"
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.5.19",
    "@types/leaflet": "^1.9.21",
    "@vueuse/integrations": "^14.2.0",
    "consola": "^3.4.2",
    "leaflet": "^1.9.4",
    "marked": "^17.0.1",
    "nuxt": "^3.15.0",
    "resend": "^6.9.1",
    "sharp": "^0.34.5",
    "simple-statistics": "^7.8.8",
    "vue": "^3.5.0"
  },
  "devDependencies": {
    "@iconify-json/mdi": "^1.2.3",
    "@nuxt/a11y": "^1.0.0-alpha.1",
    "@nuxt/icon": "^1.0.0",
    "@nuxt/image": "^2.0.0",
    "@nuxt/test-utils": "^4.0.0",
    "@nuxtjs/critters": "^0.9.0",
    "@nuxtjs/sitemap": "^7.6.0",
    "@nuxtjs/tailwindcss": "^6.12.1",
    "@pinia/nuxt": "^0.5.5",
    "@playwright/test": "^1.58.1",
    "@types/cheerio": "^0.22.35",
    "@types/node": "^25.2.0",
    "@vite-pwa/nuxt": "^1.1.0",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "@vue/test-utils": "^2.4.6",
    "@vueuse/nuxt": "^12.0.1",
    "cheerio": "^1.2.0",
    "chrome-launcher": "^1.2.1",
    "express": "^5.2.1",
    "happy-dom": "^20.6.2",
    "husky": "^9.1.7",
    "jsdom": "^28.1.0",
    "lighthouse": "^13.0.1",
    "odiff-bin": "^4.3.2",
    "open": "^11.0.0",
    "playwright": "^1.58.1",
    "tsx": "^4.21.0",
    "typescript": "^5.7.3",
    "unplugin-auto-import": "^21.0.0",
    "vitest": "^4.0.18",
    "vue-tsc": "^2.2.0"
  }
}
